record(ai,"FST:Cool:Reservoir:level") {
	field(DESC,"read channel 0")
	field(SCAN,"I/O Intr")
	field(DTYP,"asynInt32")
	field(INP,"@asynMask(ADAM-6017-IN,0,16,1000)MODBUS_DATA")
	field(PREC,"2")
	field(LINR,"LINEAR")
	field(EGUF,"100.00")
	field(EGUL,"0.0")
	field(EGU,"%")
	field(HOPR,"100.00")
	field(LOPR,"0.00")
	field(ADEL,"0.1")
	field(MDEL,"0.1")
  field(HIGH,"95")
  field(HIHI,"97")
  field(LOW,"50")
  field(LOLO,"30")
  field(HSV,"MINOR")
  field(HHSV,"MAJOR")
  field(LSV,"MINOR")
  field(LLSV,"MAJOR")
}
